<%= form_with url: '/registration', method: :post do |form| %>
  <%= form.label :contact_detail, "Enter email:" %>
  <%= form.text_field :contact_detail %>
  <br>
  <%= form.label :zipcode, "Enter zipcode:" %>
  <%= form.text_field :zipcode, value: session[:assumed_zipcode], autocomplete: "off" %> Assumed zipcode based off of your IP Address, update if necessary!
  <br>
  <%= form.submit "Register" %>
  <% flash.each do |_type, msg| %>
    <%= msg %>
  <% end %>
<% end %>
